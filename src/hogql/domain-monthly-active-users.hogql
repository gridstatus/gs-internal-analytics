-- Active users (distinct person_id) per month for a single domain. Emails must end with @domain. {{DOMAIN_LIKE}} is replaced with %@domain.
SELECT
  toStartOfMonth(timestamp) AS month,
  COUNT(DISTINCT person_id) AS active_users
FROM events
WHERE {{LOGGED_IN_USER_FILTER}}
  AND person.properties.email LIKE '{{DOMAIN_LIKE}}'
GROUP BY month
ORDER BY month ASC
